{
  "extends": "tslint-config-airbnb",
  "rules": {
    "ter-indent": false,
    "trailing-comma": false,
    "indent": [
      true,
      "tabs",
      2
    ],
    "no-increment-decrement": false,
    "triple-equals": false,
    "align": false,
    "ter-arrow-parens": false,
    "import-name": false,
    "variable-name": false,
		"no-else-after-return": false,
		"object-shorthand-properties-first": false,
		"no-parameter-reassignment": false
  }
}
